 /// Баннер
 /// В запросе на список баннеров возвращать список объектов:
   {
      "images":{/// обязательный
         "mobile":"assets/images/banner_2_mobile.png", /// полный url до изображения для мобильной версии, тип String, обязательный
         "tablet":"assets/images/banner_2_tablet.png", /// полный url до изображения для версии планшета, тип String, обязательный
         "flex":1 /// Размер баннера, тип int, диапазон {1, 2}, обязательный
      },
      "link":"https://example.com/banner_link", /// url баннера, тип String, необязательный
      "content":{/// необязательный
         "title":"А также:", /// Заголовок баннера, тип String, необязательный
         "title_font":{/// необязательный
            "size":20, /// Кегль заголовка баннера для мобильной версии, тип int, необязательный
            "tablet_size": 30, /// Кегль заголовка баннера для планшета, тип int, необязательный
            "weight":400, /// weight шрифта заголовка баннера, тип int, необязательный
            "color":"#FFFFFF", /// цвет шрифта заголовка баннера, тип String, необязательный
            "family":"Inter" /// family шрифта заголовка баннера, тип String, необязательный
         },
         "subtitle":"Выставка тыкв", /// Подзаголовок баннера, тип String, необязательный
         "subtitle_font":{/// необязательный
            "size":30,/// Кегль подзаголовка баннера для мобильной версии, тип int, необязательный
            "tablet_size": 40,  /// Кегль подзаголовка баннера для планшета, тип int, необязательный
            "weight":900, /// weight шрифта подзаголовка баннера, тип int, необязательный
            "color":"#FFFFFF", /// цвет шрифта подзаголовка баннера, тип String, необязательный
            "family":"Inter" /// family шрифта подзаголовка баннера, тип String, необязательный
         },
         "description":"Местные фермеры покажут свои самые большие тыквы. Посоревнуются чья тыква тяжелее и крепче.", /// Описание баннера, тип String, необязательный
         "description_font":{ /// необязательный
            "size":16,
            "tablet_size": 16,
            "weight":400,
            "color":"#FFFFFF",
            "family":"Arial"
         },
         "button":{
            "title":{ /// необязательный
               "text":"ЕЩЕ О ВЫСТАВКЕ", /// Текст кнопки, тип String, обязательный
               "font_sizes":{ /// обязательный
                  "tablet_size":16, /// кегль текста кнопки для мобильной версии, тип int, необязательный
                  "mobile_size":14 /// кегль текста кнопки для планшета, тип int, необязательный
               }
            },
            "text_color":"#FFFFFF", /// цвет текста кнопки, тип String, необязательный
            "border_color":"#FFFFFF",  /// цвет текста кнопки, тип String, необязательный
            "is_arrow":true /// Отображать кнопку в виде стрелки, тип bool, необязательный
         }
      }
   }


/// Карточка товара
/// Если возвращает бекенд, то в запросе возвращать список объектов:
   {
    "name": "кабачок", /// название товара, String, обязательный
    "imageUrl": "assets/images/zucchini.png", /// полный url до изображения, тип String, обязательный 
    "price": 50 /// цена товара, тип nullable double, обязательный
  }